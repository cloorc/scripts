http_proxy=http://192.168.99.1:8087 /usr/local/bin/docker daemon --insecure-registry=https://index.docker.io/v1 -D -g /var/lib/docker -H unix:// -H tcp://0.0.0.0:2376 --label provider=virtualbox --tlscacert=/var/lib/boot2docker/ca.pem --tlscert=/var/lib/boot2docker/server.pem --tlskey=/var/lib/boot2docker/server-key.pem -s aufs>docker.log 2>&1
